{
  "name": "GX-music-Daftpoe",
  "version": "0.0.2",
  "description": "GX Browser background music.",
  "manifest_version": 3,
  "action": {
    "default_title": "GX Music",
    "default_popup": "popout.html"
  },
  "icons": {
    "128": "logo.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "offscreen",
    "webNavigation",
    "tabs"
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "t-one.mp3",
        "t-two.mp3",
        "t-three.mp3",
        "t-four.mp3",
        "t-five.mp3"
      ],
      "matches": ["<all_urls>"]
    }
  ]
}